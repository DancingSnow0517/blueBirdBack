{
  "manifest_version": 3,
  "name": "Bring The Blue Bird Back",
  "description": "Let twitter web icon back to blue bird, not \"X\"",
  "version": "0.1",
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png"
  }
}